<%include ./shared/header.ejs %>
<div class="col-md-8">
	<% posts.forEach(function(post){ %>
		<div class="row">
		  	<div class="col-md-12">
		  		<div class="content-box-header">
			  		<div class="panel-title"><%=post.title%></div>
				</div>
				<div class="content-box-large box-with-header">
				  	<%=post.content%>
					<br /><br />
		  		</div>
		  	</div>
		</div>
	<% }); %>
</div>
<div class="col-md-2">
		<div class="sidebar content-box" style="display: block;">
			<ul class="sum">
	    		<li><i class="glyphicon glyphicon-user"></i> Active Users 10000000000</li>
        		<li><i class="glyphicon glyphicon-open"></i> Total Posts 2000000000</li>
        		<li><i class="glyphicon glyphicon-ok-circle"></i> Monthly SignUp 100000000</li>
    		</ul>
    	</div>
    </div>
</div>
</div>
</div>
</div>  
<% include ./shared/footer.ejs%>