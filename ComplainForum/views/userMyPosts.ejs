<% include ./shared/userHeader.ejs %>

<div class="col-md-8">
	<% posts.forEach(function(post){ %>
		<div class="row">
		  	<div class="col-md-12">
		  		<div class="content-box-header">
			  		<div class="panel-title"><%=post.title%></div>
				</div>
				<div class="content-box-large box-with-header">
				  	<%=post.content%>
					<br /><br />
				<p>Posted At: <%=post.post_date.toDateString()%></p>	
		  		</div>
		  	</div>
		</div>
	<% }); %>
</div>
</div>
</div>
</div>

<script>
 $("#posts").attr("class","current");
 $("#feed").attr("class","");
</script>  


<% include ./shared/footer.ejs %>